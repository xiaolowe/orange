<?xml version="1.0" encoding="GBK"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="race.group">    <select id="race.group.selectById" parameterType="string"        resultType="com.orange.race.entity.RaceGroupEntity">        select t.*        from t_race_group t where 1=1 and id=#{id}    </select>        <select id="race.group.selectListByRaceId" parameterType="string" resultType="com.orange.race.entity.RaceGroupEntity">        select * from t_race_group where 1=1            <if test="_parameter!=null and _parameter!=''">            and raceId=#{_parameter,jdbcType=VARCHAR}        </if>        order by createTime            </select>        <insert id="race.group.insert" parameterType="com.orange.race.entity.RaceAddonEntity"        useGeneratedKeys="true" keyProperty="id">        insert into t_race_group        (        id, createTime        <if test="raceId!=null and raceId!=''">            ,raceId        </if>        <if test="name!=null and name!=''">            ,name        </if>        <if test="maxNum!=null and maxNum!=''">            ,maxNum        </if>        <if test="price!=null">            ,price        </if>        <if test="signUpNum!=null">            ,signUpNum        </if>        <if test="type!=null and type!=''">            ,type        </if>        ) values(        id, now()        <if test="raceId!=null and raceId!=''">            ,#{raceId}        </if>        <if test="name!=null and name!=''">            ,#{name}        </if>        <if test="maxNum!=null and maxNum!=''">            ,#{maxNum}        </if>        <if test="price!=null">            ,#{price}        </if>        <if test="signUpNum!=null">            ,#{signUpNum}        </if>        <if test="type!=null and type!=''">            ,#{type}        </if>        )    </insert>        <update id="race.group.update" parameterType="com.orange.race.entity.RaceGroupEntity">        update t_race_group        set id=#{id}        <if test="name!=null and name!=''">            ,name=#{name}        </if>        <if test="maxNum!=null and maxNum!=''">            ,maxNum=#{maxNum}        </if>        <if test="price!=null">            ,price=#{price}        </if>        <if test="signUpNum!=null">            ,signUpNum=#{signUpNum}        </if>        ,updateTime = now()         where 1=1 and id=#{id}    </update>        <delete id="race.group.deleteById" parameterType="string">        delete from        t_race_group where 1=1 and id=#{id}    </delete></mapper>